{
  "name": "sixtysix",
  "version": "0.0.1",
  "description": "Sixty Six is collection of quotes so I stop forgetting what I thought and/or read.",
  "scripts": {
    "build": "cross-env ELEVENTY_ENV=production eleventy",
    "build:clean": "rimraf dist/*",
    "build:function": "netlify functions:build --src functions",
    "build:postcss": "cross-env NODE_ENV=production postcss src/_postcss/main.pcss -o src/_includes/assets/styles/main.css",
    "build:production": "run-s test build:function build:postcss build",
    "build:post": "node __task/build-post.js",
    "dev": "cross-env ELEVENTY_ENV=development eleventy --serve",
    "dev:netlify": "cross-env ELEVENTY_ENV=development netlify dev",
    "start": "npm run dev",
    "test": "jest"
  },
  "author": "Yuya Saito <hello@studiomohawk.com> (https://github.com/studiomohawk)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/frontendweekly/sixtysix.git"
  },
  "keywords": [
    "eleventy",
    "netlify"
  ],
  "bugs": {
    "url": "https://github.com/frontendweekly/sixtysix/issues"
  },
  "homepage": "https://github.com/frontendweekly/sixtysix#readme",
  "dependencies": {
    "@11ty/eleventy": "^0.11.0",
    "@11ty/eleventy-plugin-rss": "^1.0.7",
    "@11ty/eleventy-plugin-syntaxhighlight": "^3.0.1",
    "@frontendweekly/collection-postfeed": "0.0.4",
    "@frontendweekly/collection-posts": "^0.0.4",
    "@frontendweekly/molle": "^0.0.13",
    "autoprefixer": "^9.8.0",
    "cssnano": "^4.1.10",
    "dotenv": "^8.2.0",
    "node-fetch": "^2.6.0",
    "postcss": "^7.0.30",
    "postcss-cli": "^7.1.1",
    "postcss-import": "^12.0.1",
    "postcss-load-config": "^2.1.0",
    "postcss-preset-env": "^6.7.0",
    "twitter": "^1.7.1"
  },
  "devDependencies": {
    "@frontendweekly/filter-tags-to-sentence": "^0.0.4",
    "@testing-library/dom": "^7.5.6",
    "@testing-library/jest-dom": "^5.7.0",
    "cross-env": "^7.0.2",
    "eslint": "^7.0.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-import-resolver-jest": "^3.0.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jest": "^23.13.1",
    "eslint-plugin-jest-dom": "^2.1.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "eslint-plugin-testing-library": "^3.1.3",
    "husky": "^4.2.5",
    "imagemin": "^7.0.1",
    "imagemin-pngquant": "^8.0.0",
    "jest": "^26.0.1",
    "jest-date-mock": "^1.0.8",
    "jest-fetch-mock": "^3.0.3",
    "jest-runner-eslint": "^0.8.0",
    "jest-watch-select-projects": "^2.0.0",
    "lint-staged": "^10.2.2",
    "netlify-cli": "^2.51.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.5",
    "pretty-quick": "^2.0.1",
    "puppeteer": "^3.0.4",
    "qoa": "^0.2.0",
    "rimraf": "^3.0.2",
    "signale": "^1.4.0",
    "stylelint": "^13.4.0",
    "stylelint-config-prettier": "^8.0.1",
    "stylelint-config-recess-order": "^2.0.4",
